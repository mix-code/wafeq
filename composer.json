{
    "name": "mix-code/wafeq",
    "description": "Simple implementation for Wafeq API",
    "keywords": [
        "mix-code",
        "mixcode",
        "wafeq",
        "wafeq-client",
        "wafeq-api"
    ],
    "homepage": "https://github.com/mix-code/wafeq",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Mohamed Alaa El-Din",
            "email": "moaalaa16@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Hamza Omar Mohamed",
            "email": "hamzalilomar92@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Mohamed Ghareeb Ahmed",
            "email": "mohamed.ghareeb725@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "MixCode\\Wafeq\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MixCode\\Wafeq\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MixCode\\Wafeq\\WafeqServiceProvider"
            ]
        }
    },
    "require-dev": {
        "laravel/pint": "^1.21",
        "orchestra/testbench": "^9.12",
        "pestphp/pest": "^3.7"
    }
}
